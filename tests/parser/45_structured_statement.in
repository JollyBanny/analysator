function SmallAlpha(ps:string):string;
var i:integer;
begin
   for i:=1 to length(ps) do begin
      if ((ps[i] >= 'A') and (ps[i] <= 'Z')) then
    	   inc(ps[i],32)
      else
         ps[i] := ps[i];
   end;
   result:=ps;
end;

